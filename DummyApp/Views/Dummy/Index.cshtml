@model DummyApp.ViewModels.EmployeesViewModel
@{
    ViewBag.Title = "Employees";
}


    @foreach (var employee in Model.employees)
    {
        <div class="card">
            <div class="card-body" style="font-size:medium;"><a href="@Url.Action("GetEmployee", "Dummy", employee.id)">@employee.name</a></div>
        </div>
    }



